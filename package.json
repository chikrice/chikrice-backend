{
  "name": "chikrice-backend",
  "description": "backend services to power chikrice fitness app",
  "bin": "bin/createNodejsApp.js",
  "main": "src/index.js",
  "repository": "https://github.com/chikrice/chikrice-backend.git",
  "author": "Khaled Javdan",
  "version": "1.4.0",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "restart": "pm2 restart all ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node --transpile-only -r tsconfig-paths/register src/index.ts",
    "dev:strict": "cross-env NODE_ENV=development nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "test": "jest -i --colors --verbose --detectOpenHandles",
    "test:watch": "jest -i --watchAll",
    "coverage": "jest -i --coverage",
    "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.js",
    "prettier:fix": "prettier --write **/*.{js,ts}",
    "docker:prod": "docker-compose -f docker-compose.yml -f docker-compose.prod.yml up",
    "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
    "prepare": "husky install",
    "release": "release-please"
  },
  "keywords": [
    "node",
    "node.js",
    "generator",
    "express",
    "rest",
    "api",
    "mongodb",
    "mongoose",
    "es6",
    "es7",
    "es8",
    "es9",
    "jest",
    "travis",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "dependencies": {
    "@types/express": "^5.0.3",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^24.1.0",
    "axios": "^1.7.4",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "date-fns": "^3.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-mongo-sanitize": "^2.0.0",
    "express-rate-limit": "^5.0.0",
    "googleapis": "^142.0.0",
    "helmet": "^4.1.0",
    "http-status": "^1.4.0",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "5.7.7",
    "morgan": "^1.9.1",
    "node-cron": "^3.0.3",
    "openai": "^4.63.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pm2": "^5.1.0",
    "resend": "^6.0.2",
    "swagger-jsdoc": "^6.0.8",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^10.9.2",
    "validator": "^13.0.0",
    "winston": "^3.2.1",
    "xss-clean": "^0.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/jest": "^30.0.0",
    "@types/mongoose": "5.7.7",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.2",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "chikrice-types": "^1.1.3",
    "coveralls": "^3.0.7",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-security": "^1.4.0",
    "faker": "^5.1.0",
    "husky": "7.0.4",
    "jest": "^27",
    "lint-staged": "^11.0.0",
    "node-mocks-http": "^1.8.0",
    "nodemon": "^2.0.0",
    "prettier": "^3.6.2",
    "release-please": "^17.1.1",
    "supertest": "^6.0.1",
    "ts-jest": "^29.4.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
